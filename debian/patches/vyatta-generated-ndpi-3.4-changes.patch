Subject: [PATCH] nDPI-3.4 related changes for yang files

---
 yang/vyatta-ndpi-application-names-v1.yang | 44 +++++++++++++---------
 yang/vyatta-ndpi-application-types-v1.yang |  8 ++--
 2 files changed, 31 insertions(+), 21 deletions(-)

--- a/yang/vyatta-ndpi-application-names-v1.yang
+++ b/yang/vyatta-ndpi-application-names-v1.yang
@@ -55,6 +55,11 @@
 		description "Initial revision based on nDPI 3.0.0.";
 	}
 
+        revision 2020-11-02 {
+		description "Revision based on nDPI version 3.4.0 (Stable)";
+	}
+
+
 	typedef application-names {
 		/* New enums MUST only be added to the end of this list.
 		 * Old enums MAY be deprecated or obsoleted, but must not be removed.
@@ -307,6 +312,19 @@
 			enum "zattoo";
 			enum "zeromq";
 			enum "zoom";
+			enum "anydesk";
+			enum "bloomberg";
+			enum "capwap";
+			enum "discord";
+			enum "doh_dot";
+			enum "iec60870";
+			enum "microsoft365";
+			enum "nats";
+			enum "s7comm";
+			enum "soap";
+			enum "teams";
+			enum "websocket";
+			enum "zabbix";
 		}
 	}
 }
--- a/yang/vyatta-ndpi-application-types-v1.yang
+++ b/yang/vyatta-ndpi-application-types-v1.yang
@@ -55,6 +55,10 @@
 		description "Initial revision based on nDPI 3.0.0.";
 	}
 
+        revision 2020-11-02 {
+		description "Revision based on nDPI version 3.4.0 (Stable)";
+	}
+
 	typedef application-types {
 		/* New enums MUST only be added to the end of this list.
 		 * Old enums MAY be deprecated or obsoleted, but must not be removed.
@@ -85,6 +89,8 @@
 			enum "voip";
 			enum "vpn";
 			enum "web";
+			enum "connectivitycheck";
+			enum "iot_scada";
 		}
 	}
 }
